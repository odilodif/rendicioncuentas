	<subDataset name="GraficaIncorporacion" uuid="3de00d25-7ca0-44c5-a71d-1245d64eb467">
		<parameter name="Periodo" class="java.lang.Integer"/>
		<queryString>
			<![CDATA[SELECT DISTINCT
	inr.ire_entidad nombre,sum((SELECT count(rec.ire_entidad) FROM
	esq_rendicioncuentas.tbl_incoporacion_recomendaciones AS rec ,esq_rendicioncuentas.tbl_informe inf
	WHERE  rec.ire_entidad=inr.ire_entidad  and inf.inf_cod=rec.inf_cod and inf.inf_periodo=info.inf_periodo and inf.inf_estado=info.inf_estado)) as total
FROM
	esq_rendicioncuentas.tbl_incoporacion_recomendaciones AS inr ,esq_rendicioncuentas.tbl_informe info

	WHERE info.inf_cod=inr.inf_cod  and info.inf_estado='finalizado' and info.inf_periodo= $P{Periodo} 
	GROUP BY inr.ire_entidad,inr.inf_cod]]>
		</queryString>
		<field name="nombre" class="java.lang.String"/>
		<field name="total" class="java.lang.Integer"/>
	</subDataset>
	<subDataset name="Incorporacion" uuid="f9cfb001-9f5f-4745-9feb-ea8d5c1d7023">
		<parameter name="Periodo" class="java.lang.Integer"/>
		<queryString>
			<![CDATA[SELECT
	insti.inst_nom AS nombre, 
	(select lc.nombre from esq_catalogos.tbl_localizacion lc WHERE lc.id_localizacion=insti.prov_cod and lc.categoria=1) AS provincia, 
	(SELECT fu.fun_des from esq_catalogos.tbl_funcion fu WHERE fu.fun_cod=insti.inst_fun_cod) AS funcion, 
	insti.inst_tip_des AS sector, 
	info.inf_periodo AS periodo, 
	info.inf_cod, 
	insti.inst_ruc, 
	inr.ire_entidad,
	inr.ire_porcentaje
FROM
	esq_rendicioncuentas.tbl_informe AS info
	INNER JOIN
	esq_catalogos.tbl_institucion AS insti
	ON 
		insti.inst_cod = info.inst_cod
	INNER JOIN
	esq_rendicioncuentas.tbl_incoporacion_recomendaciones inr
	ON 
		info.inf_cod = inr.inf_cod
WHERE
	info.inf_estado = 'finalizado' AND
	info.inf_periodo = $P{Periodo}]]>
		</queryString>
		<field name="nombre" class="java.lang.String"/>
		<field name="provincia" class="java.lang.String"/>
		<field name="funcion" class="java.lang.String"/>
		<field name="sector" class="java.lang.String"/>
		<field name="inst_ruc" class="java.lang.String"/>
		<field name="ire_entidad" class="java.lang.String"/>
		<field name="ire_porcentaje" class="java.lang.String"/>
	</subDataset>
	<parameter name="Periodo" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[SELECT * from esq_catalogos.tbl_funcion WHERE fun_est='activo']]>
	</queryString>
