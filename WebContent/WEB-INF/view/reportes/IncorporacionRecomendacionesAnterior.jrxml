	<subDataset name="Recomendacion" uuid="465b2066-43d8-4332-bf37-4f68e7b5c7ca">
		<parameter name="Periodo" class="java.lang.Integer"/>
		<queryString>
			<![CDATA[SELECT
	insti.inst_nom AS nombre, 
	insti.inst_ruc, 
	(select lc.nombre from esq_catalogos.tbl_localizacion lc WHERE lc.id_localizacion=insti.prov_cod and lc.categoria=1) AS provincia, 
	(SELECT fu.fun_des from esq_catalogos.tbl_funcion fu WHERE fu.fun_cod=insti.inst_fun_cod) AS funcion, 
	insti.inst_tip_des AS sector, 
	info.inf_periodo AS periodo, 
	info.inf_cod, 
	ira.ca_nombre,
	ira.ca_implementado,
	ira.ca_avance
FROM
	esq_rendicioncuentas.tbl_informe AS info
	INNER JOIN
	esq_catalogos.tbl_institucion AS insti
	ON 
		insti.inst_cod = info.inst_cod
	INNER JOIN
	esq_rendicioncuentas.tbl_compromisos_informeant ira
	ON 
		info.inf_cod = ira.inf_cod
WHERE
	info.inf_estado = 'finalizado' AND
	info.inf_periodo = $P{Periodo}]]>
		</queryString>
		<field name="nombre" class="java.lang.String"/>
		<field name="inst_ruc" class="java.lang.String"/>
		<field name="provincia" class="java.lang.String"/>
		<field name="funcion" class="java.lang.String"/>
		<field name="sector" class="java.lang.String"/>
		<field name="ca_nombre" class="java.lang.String"/>
		<field name="ca_implementado" class="java.lang.String"/>
		<field name="ca_avance" class="java.lang.String"/>
	</subDataset>
	<subDataset name="GraficaRecomendacion" uuid="f3994292-d52e-4c92-a0f6-487d4ac39583">
		<parameter name="Periodo" class="java.lang.Integer"/>
		<queryString>
			<![CDATA[SELECT DISTINCT
	ian.ca_avance nombre,sum((SELECT count(an.ca_avance) FROM
	esq_rendicioncuentas.tbl_compromisos_informeant AS an ,esq_rendicioncuentas.tbl_informe inf
	INNER JOIN
	esq_catalogos.tbl_institucion AS insti
	ON 
		insti.inst_cod = inf.inst_cod
	WHERE  an.ca_avance=ian.ca_avance  and inf.inf_cod=an.inf_cod and inf.inf_periodo=info.inf_periodo and inf.inf_estado=info.inf_estado and insti.inst_fun_cod<>6 )) as total
FROM
	esq_rendicioncuentas.tbl_compromisos_informeant AS ian ,esq_rendicioncuentas.tbl_informe info
	INNER JOIN
	esq_catalogos.tbl_institucion AS insti
	ON 
		insti.inst_cod = info.inst_cod

	WHERE info.inf_cod=ian.inf_cod  and info.inf_estado='finalizado' and info.inf_periodo= $P{Periodo}  
	GROUP BY ian.ca_avance,ian.inf_cod]]>
		</queryString>
		<field name="nombre" class="java.lang.String"/>
		<field name="total" class="java.lang.Integer"/>
	</subDataset>
	<parameter name="Periodo" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[SELECT * from esq_catalogos.tbl_funcion WHERE fun_est='activo']]>
	</queryString>
