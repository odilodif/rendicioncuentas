	<subDataset name="MecanismosListado" uuid="b3ed1bc6-bbae-48c2-9080-744f5328f82d">
		<property name="com.jaspersoft.studio.data.sql.SQLQueryDesigner.sash.w1" value="200"/>
		<property name="com.jaspersoft.studio.data.sql.SQLQueryDesigner.sash.w2" value="789"/>
		<parameter name="periodo" class="java.lang.Integer"/>
		<queryString>
			<![CDATA[SELECT
insti.inst_nom nombre,
insti.provincia,
insti.funcion,
insti.inst_tip_des sector,
info.periodo,
info.inf_cod,
insti.inst_cod,
v1.meca_implementado variable1,
v2.meca_implementado variable2,
v3.meca_implementado variable3,
v4.meca_implementado variable4,
v5.meca_implementado variable5,
v6.meca_implementado variable6,
v7.meca_implementado variable7,
insti.inst_ruc
FROM	 
(SELECT 	ins.inst_cod, 
	ins.inst_nom,	
	ins.inst_ruc, 
	( SELECT fun.fun_des FROM esq_catalogos.tbl_funcion fun WHERE fun.fun_cod = ins.inst_fun_cod ) AS funcion, 
	( SELECT loc.nombre FROM esq_catalogos.tbl_localizacion loc WHERE loc.id_localizacion = ins.prov_cod ) AS provincia,
	( SELECT loc.nombre FROM esq_catalogos.tbl_localizacion loc WHERE loc.id_localizacion = ins.cant_cod ) AS canton, 
	ins.inst_tip_des, ins.inst_estado, ins.inst_fecha_registro, ins.inst_fecha_activar
	FROM	esq_catalogos.tbl_institucion ins  WHERE ins.inst_periodo=$P{periodo} )	insti  
		
	LEFT JOIN  (SELECT   f.inf_cod, f.inf_periodo periodo,f.inst_cod  FROM	esq_rendicioncuentas.tbl_informe f WHERE  
	  f.inf_estado in('finalizado') and f.inf_periodo=  $P{periodo}	) info ON info.inst_cod = insti.inst_cod	 

 LEFT JOIN (SELECT DISTINCT pp.meca_implementado,pp.inf_cod FROM esq_rendicioncuentas.tbl_mecanismo_participacion_ciudadana pp WHERE pp.meca_nombre='INSTANCIA DE PARTICIPACIÓN') v1 ON  v1.inf_cod= info.inf_cod
 
 LEFT JOIN (SELECT DISTINCT pp.meca_implementado,pp.inf_cod FROM esq_rendicioncuentas.tbl_mecanismo_participacion_ciudadana pp WHERE pp.meca_nombre='AUDIENCIA PÚBLICA') v2 ON  v2.inf_cod= info.inf_cod
 
 LEFT JOIN (SELECT DISTINCT pp.meca_implementado,pp.inf_cod FROM esq_rendicioncuentas.tbl_mecanismo_participacion_ciudadana pp WHERE pp.meca_nombre='CABILDO POPULAR') v3 ON info.inf_cod=v3.inf_cod
  
 LEFT JOIN (SELECT DISTINCT pp.meca_implementado,pp.inf_cod FROM esq_rendicioncuentas.tbl_mecanismo_participacion_ciudadana pp WHERE pp.meca_nombre='CONSEJO DE PLANIFICACIÓN LOCAL') v4 ON info.inf_cod=v4.inf_cod
 
 LEFT JOIN (SELECT DISTINCT pp.meca_implementado,pp.inf_cod FROM esq_rendicioncuentas.tbl_mecanismo_participacion_ciudadana pp WHERE pp.meca_nombre='SILLA VACÍA' )v5 ON v5.inf_cod=info.inf_cod
 
 LEFT JOIN (SELECT DISTINCT pp.meca_implementado,pp.inf_cod FROM esq_rendicioncuentas.tbl_mecanismo_participacion_ciudadana pp WHERE pp.meca_nombre='CONSEJOS CONSULTIVOS' )v6 ON v6.inf_cod=info.inf_cod 
 
LEFT JOIN (SELECT DISTINCT pp.meca_implementado,pp.inf_cod  FROM esq_rendicioncuentas.tbl_mecanismo_participacion_ciudadana pp WHERE pp.meca_nombre='OTROS')v7
 ON v7.inf_cod=info.inf_cod
 ]]>
		</queryString>
